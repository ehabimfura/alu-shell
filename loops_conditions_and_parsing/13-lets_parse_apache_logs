#!/usr/bin/env bash
#parsing apache
# Check if the log file is provided as an argument
if [ -z "$1" ]; then
  echo "Usage: $0 <apache-access-log-file>"
  exit 1
fi

LOG_FILE="$1"

# Check if the log file exists
if [ ! -f "$LOG_FILE" ]; then
  echo "Error: Log file '$LOG_FILE' not found."
  exit 1
fi

# Process the log file using awk
awk '{print $1, $9}' "$LOG_FILE"
